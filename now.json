{
  "alias": "graphql.firstmeanseverything.com",
  "env": {
    "ENGINE_API_KEY": "@fme-graphql-apollo-engine-key",
    "JWT_PASSPHRASE": "@fme-jwt-passphrase",
    "MONGO_URL": "@fme-mongo-url",
    "POSTMARK_KEY": "@fme-postmark-key",
    "STRIPE_KEY": "@fme-stripe-key"
  },
  "builds": [
    {
      "src": "src/index.js",
      "use": "@now/node-server@0.5.1"
    }
  ],
  "name": "fme-graphql",
  "routes": [
    {
      "src": "/.*",
      "dest": "src/index.js"
    }
  ],
  "version": 2
}
